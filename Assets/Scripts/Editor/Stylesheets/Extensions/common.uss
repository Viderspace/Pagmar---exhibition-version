/* Collapsed Marker State -> Corresponds to GUIStyle.normal */
JumpMarker
{
    width: 18px;
    height: 18px;
    background-image: resource("Assets/Scripts/Editor/Stylesheets/imgs/jumpTo_collapsed.png");
}

/* Active Marker State -> Corresponds to GUIStyle.onNormal */
JumpMarker:checked
{
    background-image: resource("Assets/Scripts/Editor/Stylesheets/imgs/jumpTo_normal.png");
}
